#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/recipe-app/node_modules/.pnpm/concurrently@8.2.2/node_modules/concurrently/dist/bin/concurrently.js/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/recipe-app/node_modules/.pnpm/concurrently@8.2.2/node_modules/concurrently/dist/bin/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/recipe-app/node_modules/.pnpm/concurrently@8.2.2/node_modules/concurrently/dist/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/recipe-app/node_modules/.pnpm/concurrently@8.2.2/node_modules/concurrently/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/recipe-app/node_modules/.pnpm/concurrently@8.2.2/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/recipe-app/node_modules/.pnpm/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/recipe-app/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/node_modules:/home/nxtwave/nodejs/sessions/node_modules:/home/nxtwave/nodejs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/recipe-app/node_modules/concurrently/dist/bin/concurrently.js/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/recipe-app/node_modules/concurrently/dist/bin/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/recipe-app/node_modules/concurrently/dist/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/recipe-app/node_modules/concurrently/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../concurrently/dist/bin/concurrently.js" "$@"
else 
  exec node  "$basedir/../concurrently/dist/bin/concurrently.js" "$@"
fi
